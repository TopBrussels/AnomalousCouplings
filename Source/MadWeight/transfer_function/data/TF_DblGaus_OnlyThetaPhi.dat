    <width>
        prov2=(#6+#7*dsqrt(1d0/pt(pexp))+#8*1d0/pt(pexp))
        prov5=(#19+#20*dsqrt(1d0/pt(pexp))+#21*1d0/pt(pexp))
 
        width = max(prov2, prov5) 
    </width> 
  </variable>
  <variable name='THETA'>
    <tf>
        prov1=(#1+#2*1d0/pt(p)+#3*1d0/pt(p)**2+#4*1d0/pt(p)**3+#5*1d0/pt(p)**4)

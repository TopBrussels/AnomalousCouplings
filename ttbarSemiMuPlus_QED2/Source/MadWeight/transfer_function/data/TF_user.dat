  #width type should be thin or large (thin is for energy accurate up to 5-10%)
  <variable name='PHI'>
    <tf>
      IF( ABS(eta(p(0))) .LE. 0.375) THEN 
        prov1=(#1+#2*p(0)+#3*p(0)*p(0)+#4*p(0)*p(0)*p(0)+#5*p(0)*p(0)*p(0)*p(0))
        prov2=(#6+#7*dsqrt(p(0))+#8*p(0))
        prov3=(#9+#10*p(0)+#11*p(0)*p(0)+#12*p(0)*p(0)*p(0)+#13*p(0)*p(0)*p(0)*p(0))
        prov4=(#14+#15*p(0)+#16*p(0)*p(0)+#17*p(0)*p(0)*p(0)+#18*p(0)*p(0)*p(0)*p(0))
        prov5=(#19+#20*dsqrt(p(0))+#21*p(0))
        prov6=(#22+#23*p(0)+#24*p(0)*p(0)+#25*p(0)*p(0)*p(0)+#26*p(0)*p(0)*p(0)*p(0))
 
        tf=prov2*(exp(-(p(0)-pexp(0)-prov1)**2/2d0/prov2**2))          !first (narrow) gaussian 
        tf=tf+prov5*(exp(-(p(0)-pexp(0)-prov4)**2/2d0/prov5**2))          !second (broad) gaussian
      ENDIF 
 
      IF( ABS(eta(p(0))) .GT. 0.375 .AND. ABS(eta(p(0))) .LE. 0.75) THEN 

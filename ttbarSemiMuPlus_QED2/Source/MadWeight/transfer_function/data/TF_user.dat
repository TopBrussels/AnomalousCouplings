        prov5=(#45+#46*dsqrt(p(0))+#47*p(0))
        prov6=(#48+#49*p(0)+#50*p(0)*p(0)+#51*p(0)*p(0)*p(0)+#52*p(0)*p(0)*p(0)*p(0))
 
        tf=prov2*(exp(-(p(0)-pexp(0)-prov1)**2/2d0/prov2**2))          !first (narrow) gaussian 
